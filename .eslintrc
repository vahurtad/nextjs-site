{
    "parser": "babel-eslint",
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
        "indent": ["error", 2, { "SwitchCase": 1 }],
        "linebreak-style": ["error", "unix"],
        "max-len": ["error", { "code": 120, "ignoreComments": true }],
        "quotes": ["error", "single"],
        "react/no-unused-styles": 2,
        "react/split-platform-components": 2,
        "react/no-inline-styles": 2,
        "react/display-name": 0
    }
}